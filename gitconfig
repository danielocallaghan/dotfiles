[alias]
  st = status
  ci = commit
  co = checkout
  lc = log ORIG_HEAD.. --stat --no-merges
  undo=reset --hard
  changes=diff --name-status -r
  diffstat=diff --stat -r
  lg = log --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr)%Creset' --abbrev-commit --date=relative
  last = cat-file commit HEAD
  unstage = reset HEAD
[color]
  ui = auto
[color "branch"]
  current = yellow reverse
  local = yellow
  remote = green
[color "diff"]
  meta = yellow bold
  frag = magenta bold
  old = red bold
  new = green bold
[color "status"]
  added = yellow
  changed = green
  untracked = cyan  
[user]
  name = Daniel O'Callaghan
  email = DO'Callaghan@au.westfield.com
[apply]
  whitespace = nowarn

